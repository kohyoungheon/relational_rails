<h1>Cars (only if operational:true)</h1>
<%@cars.each do |car| %>
<p>ID: <a style = "border-bottom: 2px solid green" href="/cars/<%= car.id%>"><%= car.id %></a></p>
<p>Operational: <%= car.operational %></p>
<p>Miles: <%= car.miles %></p>
<p>Color: <%= car.color %></p>
<p>Owner: <%= car.owner %></p>
<p>Garage ID: <%= car.garage_id %></p>
<p>Created: <%= car.created_at %></p>
<p>Updated: <%= car.updated_at %></p>
<a id="<%=car.id%>_edit" href = "/cars/<%=car.id%>/edit"><p>Edit Car <%=car.id%></p></a>
<%= button_to "Delete Car: #{car.id}", "/cars/#{car.id}", method: :delete, :form=>{:id=>"#{car.id}_delete"} %>
<p>----------------------------------------</p>
<%end %><br>
<span><a href="/">Back</a></span>
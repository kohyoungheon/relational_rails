<form action="/garages" method="post">
  <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
  <h3>Create a New Garage!</h3>
  <p>Indoor:</p>
  <input type='radio' name='garage[indoor]' value =true id = "option1" required/>
  <label for ="option1">True</label>
  <input type='radio' name='garage[indoor]' value =false id = "option2" />
  <label for ="option1">False</label>
  <br><br>
  <label>Slots:</label>
  <input type='number' id="slots" placeholder="Integers" name='garage[slots]' required><br><br>
  <label>City:</label>
  <input type='text' id="city" placeholder="Los Angeles" name='garage[city]' required><br><br>
  <label>Zipcode:</label>
  <input type='text' id="zipcode" placeholder="90017" name='garage[zipcode]' size = "5" maxlength="5" required><br><br>
  <label>Name:</label>
  <input type='text' id="name" placeholder="Garage Name" name='garage[name]' required><br><br>

  <input type='submit' value="Create Garage" id="submit"/>
</form>
<br>
<span><a href="/garages">Back</a></span>